{
  "name": "dkan-ui-components",
  "version": "0.0.4",
  "description": "React Components for an Open Data Catalog, used in DKAN's frontend",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmizzell/dkan-ui-components.git"
